{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier", "jest"],
  "rules": {
    "prettier/prettier": "error"
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"]
}
